programa {
  funcao inicio() {
    inteiro numero
    para(inteiro i=1;i<=10;i++){
      escreva("Digite um número: ")
    leia(numero)
      escreva(numero,"\n")
    se(numero>0){
    escreva("numero positivo\n")
    }senao se(numero<0){
      escreva("numero negativo\n")
    }senao{
      escreva("Seu numero é zero!\n")
    }
  }
 }
}
